# TTS provider: "elevenlabs" (default), "google", or "gemini"
TTS_PROVIDER=elevenlabs

# ElevenLabs (when TTS_PROVIDER=elevenlabs)
ELEVENLABS_API_KEY=your-api-key-here

# Google Cloud (when TTS_PROVIDER=google)
GOOGLE_CLOUD_API_KEY=your-api-key-here

# Gemini TTS (when TTS_PROVIDER=gemini)
GEMINI_API_KEY=your-api-key-here
GEMINI_TTS_MODEL=gemini-2.5-pro-preview-tts
GEMINI_TTS_VOICE=Kore
