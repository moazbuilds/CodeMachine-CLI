{
  "name": "codemachine",
  "version": "0.8.0",
  "description": "Multi-Agent Workflow Orchestration.",
  "license": "Apache-2.0",
  "author": "CodeMachine Contributors",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/moazbuilds/CodeMachine-CLI.git"
  },
  "homepage": "https://github.com/moazbuilds/CodeMachine-CLI#readme",
  "bugs": {
    "url": "https://github.com/moazbuilds/CodeMachine-CLI/issues"
  },
  "keywords": [
    "ai",
    "agents",
    "multi-agent",
    "workflow",
    "orchestration",
    "automation",
    "cli",
    "llm",
    "code-generation"
  ],
  "type": "module",
  "bin": {
    "codemachine": "bin/codemachine.js",
    "cm": "bin/codemachine.js"
  },
  "files": [
    "bin/",
    "config/",
    "prompts/",
    "templates/",
    "README.md",
    "LICENSE"
  ],
  "engines": {
    "node": ">=20.10.0",
    "bun": "1.3.3"
  },
  "optionalDependencies": {
    "codemachine-linux-x64": "0.8.0",
    "codemachine-linux-arm64": "0.8.0",
    "codemachine-darwin-arm64": "0.8.0",
    "codemachine-darwin-x64": "0.8.0",
    "codemachine-windows-x64": "0.8.0"
  },
  "scripts": {
    "_comment_dev": "Development: Run from source (no build needed)",
    "dev": "DEBUG=true bun --conditions=browser src/runtime/cli-setup.ts",
    "debug": "DEBUG=true bun --conditions=browser src/runtime/cli-setup.ts",
    "start": "bun --conditions=browser src/runtime/cli-setup.ts",
    "_comment_build": "Production: Build platform-specific executables",
    "build": "bun scripts/build.ts",
    "_comment_quality": "Code Quality: Linting, testing, formatting",
    "lint": "eslint --max-warnings=0 \"src/**/*.{ts,tsx}\"",
    "test": "bun test",
    "test:watch": "bun test --watch",
    "test:coverage": "bun test --coverage",
    "format": "prettier --write .",
    "typecheck": "tsc --noEmit",
    "_comment_telemetry": "Telemetry: Import and analyze traces/logs",
    "import-telemetry": "bun scripts/import-telemetry.ts",
    "_comment_release": "Release: Publishing and validation",
    "prepare": "husky install",
    "release": "bun scripts/publish.ts --tag latest",
    "publish:all": "bun scripts/publish.ts --tag latest --dry-run"
  },
  "dependencies": {
    "@clack/prompts": "^0.11.0",
    "@modelcontextprotocol/sdk": "^1.0.0",
    "@opentelemetry/api": "^1.9.0",
    "@opentelemetry/api-logs": "^0.211.0",
    "@opentelemetry/exporter-logs-otlp-http": "^0.211.0",
    "@opentelemetry/exporter-metrics-otlp-http": "^0.211.0",
    "@opentelemetry/exporter-trace-otlp-http": "^0.211.0",
    "@opentelemetry/exporter-zipkin": "^2.5.0",
    "@opentelemetry/resources": "^2.5.0",
    "@opentelemetry/sdk-logs": "^0.211.0",
    "@opentelemetry/sdk-metrics": "^2.5.0",
    "@opentelemetry/sdk-node": "^0.211.0",
    "@opentelemetry/sdk-trace-base": "^2.5.0",
    "@opentelemetry/semantic-conventions": "^1.39.0",
    "@opentui/core": "0.1.73",
    "@opentui/solid": "^0.1.73",
    "chalk": "^5.6.2",
    "commander": "^14.0.1",
    "fuzzysort": "^3.1.0",
    "proper-lockfile": "^4.1.2",
    "solid-js": "^1.9.10",
    "update-notifier": "^7.3.1",
    "zod": "^3.23.0"
  },
  "devDependencies": {
    "@eslint/js": "^9.36.0",
    "@types/babel__core": "^7.20.5",
    "@types/bun": "^1.3.4",
    "@types/node": "^25.0.0",
    "@types/proper-lockfile": "^4.1.4",
    "@types/update-notifier": "^6.0.8",
    "@typescript-eslint/eslint-plugin": "^8.49.0",
    "@typescript-eslint/parser": "^8.49.0",
    "eslint": "^9.0.0",
    "eslint-config-prettier": "^10.1.8",
    "eslint-import-resolver-typescript": "^4.4.4",
    "eslint-plugin-import": "^2.29.1",
    "husky": "^9.0.7",
    "prettier": "^3.7.4",
    "typescript": "^5.4.5",
    "typescript-eslint": "^8.49.0"
  }
}
